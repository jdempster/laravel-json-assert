{
    "name": "jdempster/laravel-json-assert",
    "description": "Laravel JSON schema assertion",
    "license": "MIT",
    "authors": [
        {
            "name": "James Dempster",
            "email": "letssurf@gmail.com",
            "homepage": "http://jdempster.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/support": "^5.6 || ^5.7 || ^5.8",
        "opis/json-schema": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jdempster\\JsonAssert\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Jdempster\\JsonAssert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
